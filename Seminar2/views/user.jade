extends master-template
block content
  .container
    .col-md-12.page
        .page-header
          h2 #{userprofile['user_name']}
        p.lead #{userprofile['user_email']}
        h3 Korisnik je dodao:
        ul
          for location in isauthor
            li
              a(href='#{baseUrl}/location/#{location['location_id']}') #{location['location_name']}